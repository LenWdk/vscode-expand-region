{
  "name": "expand-region-dart",
  "displayName": "expand-region-dart",
  "description": "expand selection , It works similar to ExpandRegion for Emacs and “Structural Selection” (Control-W) in the JetBrains IDE's (i.e. IntelliJ IDEA).",
  "version": "0.1.2",
  "publisher": "lenwdk",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/lenwdk/vscode-expand-region.git"
  },
  "homepage": "https://github.com/lenwdk/vscode-expand-region/blob/master/README.md",
  "engines": {
    "vscode": "^1.1.5"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:expand_region_dart"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "expand_region_dart",
        "title": "Expand Region with Dart"
      },
      {
        "command": "undo_expand_region_dart",
        "title": "Undo Expand Region with Dart"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "mocha --ui tdd --recursive \"out/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^7.0.7",
    "eslint": "^3.6.0",
    "mocha": "^3.5.0",
    "typescript": "^4.1.3",
    "vscode": "^1.1.5"
  }
}